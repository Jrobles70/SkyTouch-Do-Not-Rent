{
  "manifest_version": 2,

  "name": "Do Not Rent",
  "description": "This extension checks reservation page for a guest on the do not rent list",
  "version": "1.0",

"content_scripts": [
  {
    "matches": [
      "https://www.skytouchhos.com/pms/ReserveRoom.do",
      "https://www.skytouchhos.com/pms/ViewArrivalsList.do",
      "https://www.skytouchhos.com/pms/ViewArrivalsList.init"
    ],
    "js": ["content.js", "jquery-3.1.1.js"]
  }
]
}
